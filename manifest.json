{
    "name": "Sweet for Chrome",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Remove all conversations from the Global page of Alpha.app.net",
    "content_scripts": [
        {
            "matches": [ "http://alpha.app.net/*", "https://alpha.app.net/*" ],
            "js":      [ "jquery-1.8.0.min.js", "sweet.js" ]
        }
    ],
    "permissions": [
        "http://alpha.app.net/*",
        "https://alpha.app.net/*"
    ]
        
}